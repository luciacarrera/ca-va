<div class="flex gap-1 items-end">
  {{#if icon}}
    <img src="{{icon.src}}" alt="{{icon.alt}}" class="w-4 h-4 pb-0.5" />
  {{/if}}
  <p class="font-mono tracking-tightest text-xs">{{#if href}}<a
        href="{{href}}"
        class="underline underline-offset-2"
      >{{value}}</a>{{else}}{{value}}{{/if}}</p>
</div>